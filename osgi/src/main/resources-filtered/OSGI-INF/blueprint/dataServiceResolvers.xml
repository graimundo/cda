<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

  <reference id="dataServiceMetaFactory" interface="org.pentaho.di.trans.dataservice.IDataServiceMetaFactory" />
  <reference id="dataServiceContext" interface="org.pentaho.di.trans.dataservice.Context"/>

  <bean id="cdaDataServiceResolverBean" class="org.pentaho.ctools.cda.connections.dataservices.CdaDataServiceResolver">
    <argument ref="dataServiceMetaFactory"/>
    <argument ref="dataServiceContext"/>
    <argument value="${repos.repoName}"/>
    <argument value="${repos.user}"/>
    <argument value="${repos.password}"/>
  </bean>

  <service ref="cdaDataServiceResolverBean" interface="org.pentaho.di.trans.dataservice.resolvers.DataServiceResolver"
           ranking="3"/>
</blueprint>
